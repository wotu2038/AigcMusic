# 阿里云RDS的MySQL配置
# ============================================
# 开发环境配置（当前使用）
DB_ENGINE=django.db.backends.mysql
DB_NAME=your-db-name-dev
DB_USER=your-db-user
DB_PASSWORD=your-db-password
DB_HOST=your-rds-host.mysql.rds.aliyuncs.com
DB_PORT=3306

# 生产环境配置（取消注释以使用生产环境）
# DB_ENGINE=django.db.backends.mysql
# DB_NAME=your-db-name-prod
# DB_USER=your-db-user
# DB_PASSWORD=your-db-password
# DB_HOST=your-production-rds-host.mysql.rds.aliyuncs.com
# DB_PORT=3306

# 阿里云OSS配置
# ============================================
# 开发环境配置（当前使用）
OSS_ACCESS_KEY_ID=your-oss-access-key-id
OSS_ACCESS_KEY_SECRET=your-oss-access-key-secret
OSS_ENDPOINT=https://oss-cn-beijing.aliyuncs.com
OSS_BUCKET_NAME=your-oss-bucket-name-dev

# 生产环境配置（取消注释以使用生产环境）
# OSS_ACCESS_KEY_ID=your-oss-access-key-id
# OSS_ACCESS_KEY_SECRET=your-oss-access-key-secret
# OSS_ENDPOINT=https://oss-cn-beijing.aliyuncs.com
# OSS_BUCKET_NAME=your-oss-bucket-name-prod

# Redis配置
# ============================================
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password
REDIS_DB=0

# Django配置
# ============================================
# 开发环境配置（当前使用）
DJANGO_SECRET_KEY=your-secret-key-change-in-production
DJANGO_DEBUG=True
DJANGO_ALLOWED_HOSTS=localhost,127.0.0.1,0.0.0.0

# 生产环境配置（取消注释以使用生产环境）
# DJANGO_SECRET_KEY=your-production-secret-key-change-this
# DJANGO_DEBUG=False
# DJANGO_ALLOWED_HOSTS=your-production-ip,localhost,127.0.0.1

# CORS配置
# ============================================
# 开发环境配置（当前使用）
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:80

# 生产环境配置（取消注释以使用生产环境）
# CORS_ALLOWED_ORIGINS=http://your-production-ip:8025,https://your-production-ip:8443

# 服务端口配置
# ============================================
# 开发环境配置（当前使用）
WEB_PORT=8025
HTTPS_PORT=8443

# 生产环境配置（取消注释以使用生产环境）
# WEB_PORT=8025
# HTTPS_PORT=8443

# 阿里万相（通义万相）配置
# ============================================
ALIBABA_WANXIANG_API_KEY=your-wanxiang-api-key
ALIBABA_WANXIANG_API_SECRET=your-wanxiang-api-secret
ALIBABA_WANXIANG_ENDPOINT=dashscope.aliyuncs.com
ALIBABA_WANXIANG_MODEL_IMAGE=wanx-v1
ALIBABA_WANXIANG_MODEL_TEXT=qwen-turbo
ALIBABA_WANXIANG_MODEL_VIDEO=wan2.2-i2v-plus

# 前端API配置（可选，前端有默认值）
# ============================================
# 开发环境配置（当前使用）
VITE_API_URL=http://localhost:8025/api

# 生产环境配置（取消注释以使用生产环境）
# VITE_API_URL=http://your-production-ip:8025/api
# 或者如果使用HTTPS：
# VITE_API_URL=https://your-production-ip:8443/api
